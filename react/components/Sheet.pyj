import react
import orm.models as models

View = react.get_components('View')

Card = react.get_framework_components('Card')

class Sheet(React.Component):

    def __init__(self):
        return

    def render(self):

        return (
            Card (style={'minHeight': 250, 'margin': 8, 'marginTop': 20, 'flexWrap': 'wrap', 'flexDirection': 'row'},
                self.props.children
            )
        )

react.register_component(Sheet)
